{
  "name": "worldview-platform",
  "version": "1.0.0",
  "description": "A complete worldview publishing platform",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
